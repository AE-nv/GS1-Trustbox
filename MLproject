name: GS1-Trustbox

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      n_jobs: {type: int, default: 2}
      random_state: {type: int, default: 42}
      bucket: {type: string, default: ''}
      n_cpus: {type: int, default: 1}
    command: "python GS1_predMod_v3.py {n_jobs} {random_state} {bucket} {n_cpus}"
